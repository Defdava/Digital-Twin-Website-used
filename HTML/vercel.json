{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": ".*\\.(css|js|png|jpg|jpeg|gif|svg|ico)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/login", "dest": "/login.html" },
    { "src": "/register", "dest": "/register.html" },
    { "src": "/forgot-password", "dest": "/forgot-password.html" },
    { "src": "/live-overview", "dest": "/live-overview.html" },
    { "src": "/previous-analysis", "dest": "/previous-analysis.html" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }  ‚Üê fallback benar
  ]
}
